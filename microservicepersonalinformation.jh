/**
 * 规范流程。
 */
entity StandardProcedures {
  isActive Boolean required,
  specification TextBlob required,
  userLogin String required  // 直接存储用户登录名
  userUUID String required  // 直接存储用户UUID
}

// 不使用关系，改为存储字段
